class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        m={}
        b=[]
        r=0
        for i in range(0,len(nums)):
            r=target -nums[i]
            if r in m:
                b.append(m[r])
                b.append(i)

            else:
                m[nums[i]]=i
        return b
