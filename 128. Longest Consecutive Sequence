class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        s=set(nums)
        lon=0
        for i in s:
            if i-1 not in s:
                curst=i
                strike=1
                while curst+1 in s:
                    curst+=1
                    strike+=1
                lon=max(lon,strike)
        return lon
        
