class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        s.sort()
        g.sort()
        cnt=0
        i=len(g)-1
        while i>-1:
            if not s:
                return cnt
            if s[-1]>=g[i]:
                cnt+=1
                i-=1
                s.pop()
            else:
                i-=1
        return cnt
