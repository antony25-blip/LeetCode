class Solution:
    def maximumDifference(self, nums: List[int]) -> int:
        maxn=0
        d=0
        for i in range(len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i]>nums[j]:
                    break
                else:
                    d=nums[j]-nums[i]
                    maxn=max(d,maxn)
        if maxn>0:
            return maxn
        else:
            return -1
        
