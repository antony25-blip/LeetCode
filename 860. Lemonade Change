class Solution:
    def lemonadeChange(self, bills: List[int]) -> bool:
        if bills[0]!=5:
            return False
        m={}
        m[5]=0
        m[10]=0
        m[20]=0
        for i in bills:
            if i == 5:
                m[5]+=1
            elif i==10:
                if m[5]<1:
                    return False
                else:
                    m[5]-=1
                    m[10]+=1
            elif i == 20:
                if m[10]>=1 and m[5]>=1:
                    m[10]-=1
                    m[5]-=1
                    m[20]+=1
                elif m[5]>=3:
                    m[5]-=3
                    m[20]+=1
                else:
                    return False
        return True

        
